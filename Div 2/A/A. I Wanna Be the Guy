#include<bits/stdc++.h>
using namespace std;
int main()
{
	int n;
	cin>>n;
	int x,y;
	cin>>x;
	vector<int> v;
	for(int i=0;i<x;i++)
	{
		int a;
		cin>>a;
		v.push_back(a);
	}
	cin>>y;
	for(int i=0;i<y;i++)
	{
		int a;
		cin>>a;
		v.push_back(a);
	}
	set<int> s(v.begin(),v.end());
	if(s.size()==n)
	cout<<"I become the guy."<<endl;
	else
	cout<<"Oh, my keyboard!"<<endl;
}
